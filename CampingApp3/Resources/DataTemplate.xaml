<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CampingApp3.ViewModels"
                    xmlns:view="clr-namespace:CampingApp3.Views.UserControls">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MapVM}">
        <view:Map />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginVM}">
        <view:Login />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AccountVM}">
        <view:Account />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RegisterVM}">
        <view:Register />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminVM}">
        <view:Admin />
    </DataTemplate>

</ResourceDictionary>